) => {\n      if(com){\n        setArr(com);\n      }\n    });\n    return (\n      <div>\n        {arr&& arr.map(comments => (\n          <PostItem comments={comments} />\n        ))}\n        {com && !com.length && <div>댓글이 없습니다.</div> } \n      </div>\n    );\n};\n\nexport default ShowComments;","/home/jobchae/AIhomeTraining_web/Listener/frontend/src/components/common/Comment.js",["306"],"import React from 'react';\nimport styled, { css } from 'styled-components';\nimport { Link } from 'react-router-dom';\nimport palette from '../../lib/style/palette';\n\nconst SubInfoBlock = styled.div`\n  ${props =>\n    props.hasMarginTop &&\n    css`\n      margin-top: 1rem;\n    `}\n  color: ${palette.gray[6]};\n  /* span 사이에 가운뎃점 문자 보여주기*/\n  span + span:before {\n    color: ${palette.gray[4]};\n    padding-left: 0.25rem;\n    padding-right: 0.25rem;\n    content: '\\\\B7'; /* 가운뎃점 문자 */\n  }\n`;\nconst Div = styled.div`\n  padding-top: 5%;\n  color: black;\n`;\n\nconst Comment = ({ username, level, text, publishedDate, hasMarginTop }) => {\n    return (\n      <SubInfoBlock hasMarginTop={hasMarginTop}>\n        <span>\n          <b>\n            {level}<h1> </h1>\n            {username} \n            <span> {new Date(publishedDate).toLocaleDateString()}</span>\n          </b>\n          <Div>{text}</Div>\n        </span>\n      \n      </SubInfoBlock>\n    );\n  };\n  \n  export default Comment;\n  ","/home/jobchae/AIhomeTraining_web/Listener/frontend/src/pages/About.js",[],"/home/jobchae/AIhomeTraining_web/Listener/frontend/src/containers/exercise/ExerciseContainer.js",[],{"ruleId":"307","replacedBy":"308"},{"ruleId":"309","replacedBy":"310"},{"ruleId":"311","severity":1,"message":"312","line":6,"column":8,"nodeType":"313","messageId":"314","endLine":6,"endColumn":14},{"ruleId":"311","severity":1,"message":"315","line":1,"column":8,"nodeType":"313","messageId":"314","endLine":1,"endColumn":13},{"ruleId":"311","severity":1,"message":"316","line":1,"column":17,"nodeType":"3